{
    "name": "genial-framework/skeleton-application",
    "description": "A powerful MVC framework design for speed and flexibility.",
    "keywords": [
        "php7-framework",
        "elegant-mvc-framework",
        "secure-coding",
        "skeleton-application"
    ],
    "license": "",
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish0820@outlook.com",
            "homepage": "https://github.com/Nenglish7"
        }
    ],
    "autoload": {
        "psr-4": {
            "Genial\\Controller\\": "./app/Controller"
        }
    },
    "require": {
        "php": "^7.2",
        "psr/log": "*",
        "symfony/console": "^4",
        "symfony/process": "^4",
        "symfony/event-dispatcher": "^4",
        "symfony/lock": "^4",
        "ramsey/uuid": "^3",
        "moontoast/math": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "vimeo/psalm": "^1"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
